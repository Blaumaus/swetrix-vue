{
  "name": "swetrix-vue",
  "description": "Swetrix Tracking integration for Vue v3",
  "author": "Le Minh Tri <ansidev@gmail.com>",
  "license": "MIT",
  "version": "1.1.0-rc.0",
  "type": "module",
  "packageManager": "pnpm@7.9.3",
  "homepage": "https://github.com/ansidev/swetrix-vue",
  "funding": [
    {
      "type": "paypal",
      "url": "https://paypal.me/ansidev"
    },
    {
      "type": "kofi",
      "url": "https://ko-fi.com/ansidev"
    },
    {
      "type": "buymeacoffee",
      "url": "https://buymeacoffee.com/ansidev"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ansidev/swetrix-vue"
  },
  "bugs": {
    "url": "https://github.com/ansidev/swetrix-vue/issues"
  },
  "keywords": [
    "swetrix",
    "analytics",
    "monitoring",
    "metrics",
    "privacy",
    "vue",
    "vuejs"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint . --ext .ts --ignore-path .gitignore src",
    "lint:fix": "eslint --ext .ts --ignore-path .gitignore --fix src",
    "build": "tsup lib/index.ts --dts --format cjs,esm",
    "build:example": "pnpm run build && vue-tsc --noEmit && vite build",
    "dev": "pnpm run build -- --watch && vite",
    "preview": "vite preview",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:pre": "standard-version --prerelease rc"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "dependencies": {
    "swetrix": "^1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@vitejs/plugin-vue": "^3.0.3",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "standard-version": "^9.5.0",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vite-plugin-vue-markdown": "^0.21.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.40.1"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  }
}
